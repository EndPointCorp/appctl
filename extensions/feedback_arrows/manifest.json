{
  "manifest_version": 2,

  "name": "Feedback Arrows",
  "description": "This extension shows feedback arrows while using spacenav",
  "version": "1.0",
  "background": {
    "page": "feedback_arrows_threejs.html"
  },
  "content_scripts": [{
    "matches": ["*://*.google.com/maps/*"],
    
    "js": [
      "lib/three.min.js",
      "lib/OBJloader.js",	
      "lib/eventemitter2.min.js",
      "lib/roslib.min.js",
      "apply.js"
      ],
    "all_frames": true
  }],

  "permissions": [
    "<all_urls>",
    "tabs",
    "webRequest",
    "webNavigation",
    "management"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "update_url": "http://lg-head/extensions/onboard-upd.xml"
}